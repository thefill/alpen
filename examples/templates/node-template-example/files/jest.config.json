{
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "{{escaped-path}}\/(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?|tsx?)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "testEnvironment": "node"
}
