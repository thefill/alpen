{
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/src/.+\\.e2e\\.ts$",
    "testPathIgnorePatterns": [
        "/node_modules/",
        "/examples/",
        "/templates/"
    ],
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "testEnvironment": "node"
}
